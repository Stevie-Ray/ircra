{
  "name": "ircra",
  "version": "2.0.2",
  "description": "IRCRA Climbing Grade Conversion Utility",
  "repository": {
    "type": "git",
    "url": "https://github.com/wulfmann/ircra.git"
  },
  "main": "dist/ircra.js",
  "module": "dist/ircra.mjs",
  "unpkg": "dist/ircra.umd.js",
  "source": "src/ircra.ts",
  "author": "Joe Snell <joepsnell@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "microbundle --compress --raw",
    "dev": "microbundle watch",
    "bump": "standard-version",
    "release": "yarn bump && git push --follow-tags origin master"
  },
  "keywords": [
    "rock climbing",
    "bouldering",
    "climbing grades",
    "grades",
    "ircra"
  ],
  "devDependencies": {
    "microbundle": "^0.11.0",
    "standard-version": "^7.0.0"
  }
}
